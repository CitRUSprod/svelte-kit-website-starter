        "tsup": "8.3.6",
        "type-fest": "4.34.1",
        "typescript": "5.7.3"
    },
    "dependencies": {
        "@local/constants": "workspace:*",
        "@local/utils": "workspace:*",
        "lodash-es": "4.17.21",
        "tslib": "2.8.1"
    }
