{
    "name": "api",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently -k -n build,types -c blue,magenta -p [{name}] pnpm:dev:build pnpm:dev:check-types",
        "dev:build": "tsup --config tsup/dev.ts --watch --onSuccess \"NODE_OPTIONS=--enable-source-maps pnpm start\"",
        "dev:check-types": "pnpm check-types -w --preserveWatchOutput",
        "build": "tsup --config tsup/prod.ts",
        "start": "node dist/main",
        "check-types": "tsc --pretty --noEmit",
        "prisma": "dotenv -e ../../.env -- prisma",
        "prisma:generate": "pnpm prisma generate",
        "prisma:studio": "pnpm prisma studio",
        "prisma:migrations": "pnpm prisma migrate",
        "prisma:migrations:create": "pnpm prisma:migrations dev",
        "prisma:migrations:run": "pnpm prisma:migrations deploy",
        "prisma:migrations:reset": "pnpm prisma:migrations reset",
        "prisma:seeds:run": "pnpm prisma db seed",
        "prepare": "pnpm prisma:generate"
    },
    "prisma": {
        "schema": "src/prisma/schema.prisma",
        "seed": "esr src/prisma/seeds.ts"
    },
    "devDependencies": {
        "@local/configs": "workspace:*",
        "@total-typescript/ts-reset": "0.5.1",
        "@types/fs-extra": "11.0.4",
        "@types/node": "20.9.3",
        "@types/nodemailer": "6.4.14",
        "@types/sharp": "0.31.1",
        "@types/uuid": "9.0.7",
        "concurrently": "8.2.2",
        "defu": "6.1.3",
        "dotenv-cli": "7.3.0",
        "dotenv-expand": "10.0.0",
        "esbuild-runner": "2.2.2",
        "prisma": "5.6.0",
        "tsup": "8.0.1",
        "type-fest": "4.8.2",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "@fastify/auth": "4.4.0",
        "@fastify/cookie": "9.2.0",
        "@fastify/jwt": "7.2.3",
        "@fastify/multipart": "8.0.0",
        "@fastify/static": "6.12.0",
        "@fastify/swagger": "8.12.0",
        "@fastify/swagger-ui": "1.10.1",
        "@prisma/client": "5.6.0",
        "@segment/ajv-human-errors": "2.11.3",
        "@sinclair/typebox": "0.31.28",
        "ajv": "8.12.0",
        "ajv-formats": "2.1.1",
        "ajv-keywords": "5.1.0",
        "argon2": "0.31.2",
        "dotenv": "16.3.1",
        "fastify": "4.24.3",
        "fastify-plugin": "4.5.1",
        "fastify-socket.io": "4.0.0",
        "fs-extra": "11.1.1",
        "http-errors-enhanced": "1.1.2",
        "nodemailer": "6.9.7",
        "sharp": "0.32.6",
        "socket.io": "4.7.2",
        "socketio-jwt-auth": "0.2.1",
        "tslib": "2.6.2",
        "uuid": "9.0.1"
    }
}
