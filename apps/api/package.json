{
    "name": "api",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently -k -n i18n,build,types -c green,blue,magenta -p [{name}] pnpm:dev:i18n pnpm:dev:build pnpm:dev:check-types",
        "dev:i18n": "typesafe-i18n",
        "dev:build": "tsup --config tsup/dev.ts --watch --onSuccess \"NODE_OPTIONS=--enable-source-maps pnpm start\"",
        "dev:check-types": "pnpm check-types -w --preserveWatchOutput",
        "i18n": "typesafe-i18n --no-watch",
        "build": "tsup --config tsup/prod.ts",
        "start": "node dist/main",
        "check-types": "tsc --pretty --noEmit",
        "prisma": "dotenv -e ../../.env -- prisma",
        "prisma:generate": "pnpm prisma generate",
        "prisma:studio": "pnpm prisma studio",
        "prisma:migrations": "pnpm prisma migrate",
        "prisma:migrations:create": "pnpm prisma:migrations dev",
        "prisma:migrations:run": "pnpm prisma:migrations deploy",
        "prisma:migrations:reset": "pnpm prisma:migrations reset",
        "prisma:seeds:run": "pnpm prisma db seed",
        "prepare": "pnpm prisma:generate",
        "typesafe-i18n": "typesafe-i18n"
    },
    "prisma": {
        "schema": "src/prisma/schema.prisma",
        "seed": "esno src/prisma/seeds.ts"
    },
    "devDependencies": {
        "@local/configs": "workspace:*",
        "@total-typescript/ts-reset": "0.5.1",
        "@types/fs-extra": "11.0.4",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.10.1",
        "@types/nodemailer": "6.4.14",
        "@types/sharp": "0.31.1",
        "@types/socketio-jwt-auth": "0.0.5",
        "@types/uuid": "9.0.8",
        "concurrently": "8.2.2",
        "defu": "6.1.4",
        "dotenv-cli": "7.4.1",
        "dotenv-expand": "11.0.6",
        "esno": "4.7.0",
        "prisma": "5.13.0",
        "tsup": "8.0.2",
        "type-fest": "4.18.1",
        "typescript": "5.4.5"
    },
    "dependencies": {
        "@fastify/auth": "5.0.1",
        "@fastify/cookie": "11.0.1",
        "@fastify/jwt": "9.0.1",
        "@fastify/multipart": "9.0.1",
        "@fastify/static": "8.0.3",
        "@fastify/swagger": "9.4.0",
        "@fastify/swagger-ui": "5.1.0",
        "@local/constants": "workspace:*",
        "@local/schemas": "workspace:*",
        "@local/utils": "workspace:*",
        "@prisma/client": "5.13.0",
        "arctic": "1.8.0",
        "argon2": "0.40.1",
        "axios": "1.6.8",
        "dotenv": "16.4.5",
        "fastify": "5.1.0",
        "fastify-plugin": "5.0.1",
        "fastify-socket.io": "5.1.0",
        "fastify-type-provider-zod": "4.0.2",
        "fs-extra": "11.2.0",
        "http-errors-enhanced": "2.0.8",
        "lodash-es": "4.17.21",
        "nodemailer": "6.9.13",
        "sharp": "0.33.3",
        "socket.io": "4.7.5",
        "socketio-jwt-auth": "0.2.1",
        "tslib": "2.6.2",
        "typesafe-i18n": "5.26.2",
        "uuid": "9.0.1"
    }
}
