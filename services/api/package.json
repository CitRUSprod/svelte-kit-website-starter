{
    "name": "api",
    "private": true,
    "scripts": {
        "dev": "node configs/estrella/dev",
        "build": "node configs/estrella/prod",
        "start": "node dist/app",
        "validate": "tsc --noEmit",
        "ts": "ts-node -r tsconfig-paths/register -T",
        "typeorm": "pnpm ts -- node_modules/typeorm/cli.js -f configs/typeorm/index.js",
        "typeorm-seeding": "pnpm ts -- node_modules/typeorm-seeding/dist/cli.js -n configs/typeorm/index.js",
        "db:migrations:run": "pnpm typeorm -- migration:run",
        "db:migrations:revert": "pnpm typeorm -- migration:revert",
        "db:seeds:run": "pnpm typeorm-seeding -- seed"
    },
    "devDependencies": {
        "@types/faker": "5.5.9",
        "@types/http-errors": "1.8.1",
        "@types/node": "17.0.5",
        "@types/nodemailer": "6.4.4",
        "@types/uuid": "8.3.3",
        "defu": "5.0.0",
        "dotenv": "10.0.0",
        "esbuild-node-externals": "1.4.1",
        "esbuild-plugin-clean": "0.5.0",
        "estrella": "1.4.1",
        "source-map-support": "0.5.21",
        "ts-node": "10.4.0",
        "tsconfig-paths": "3.12.0",
        "type-fest": "2.8.0",
        "typeorm-seeding": "1.6.1",
        "typescript": "4.5.4"
    },
    "dependencies": {
        "argon2": "0.28.3",
        "fastify": "3.25.2",
        "fastify-auth": "1.1.0",
        "fastify-cookie": "5.4.0",
        "fastify-jwt": "4.1.3",
        "fastify-socket.io": "3.0.0",
        "fastify-typeorm-plugin": "2.2.0",
        "fastify-yup-schema": "1.0.4",
        "http-errors": "2.0.0",
        "nodemailer": "6.7.2",
        "pg": "8.7.1",
        "reflect-metadata": "0.1.13",
        "socket.io": "4.4.0",
        "socketio-jwt-auth": "0.2.1",
        "typeorm": "0.2.41",
        "uuid": "8.3.2"
    }
}
