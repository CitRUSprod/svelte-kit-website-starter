{
    "name": "api",
    "private": true,
    "scripts": {
        "dev": "concurrently -k -n b,v -c blue,magenta -p [{name}] pnpm:dev:build pnpm:dev:validate",
        "dev:build": "node esbuild/dev",
        "dev:validate": "pnpm validate -w --preserveWatchOutput",
        "build": "node esbuild/prod",
        "start": "node dist/main",
        "validate": "tsc --pretty --noEmit",
        "prisma": "dotenv -e ../../.env -- prisma",
        "prisma:generate": "pnpm prisma generate",
        "prisma:studio": "pnpm prisma studio",
        "prisma:migrations": "pnpm prisma migrate",
        "prisma:migrations:create": "pnpm prisma:migrations dev",
        "prisma:migrations:run": "pnpm prisma:migrations deploy",
        "prisma:migrations:reset": "pnpm prisma:migrations reset",
        "prisma:seeds:run": "pnpm prisma db seed",
        "prepare": "pnpm prisma:generate"
    },
    "prisma": {
        "schema": "src/prisma/schema.prisma",
        "seed": "ts-node -T src/prisma/seeds.ts"
    },
    "devDependencies": {
        "@swc/core": "1.3.18",
        "@types/fs-extra": "9.0.13",
        "@types/http-errors": "2.0.1",
        "@types/node": "18.11.9",
        "@types/nodemailer": "6.4.6",
        "@types/sharp": "0.31.0",
        "@types/uuid": "8.3.4",
        "concurrently": "7.5.0",
        "defu": "6.1.1",
        "dotenv-cli": "6.0.0",
        "dotenv-expand": "9.0.0",
        "esbuild": "0.14.54",
        "esbuild-plugin-clean": "1.0.0",
        "esbuild-plugin-decorator": "0.4.0",
        "esbuild-plugin-node-externals": "1.0.0",
        "esbuild-plugin-run": "0.3.0",
        "prisma": "4.6.1",
        "source-map-support": "0.5.21",
        "ts-node": "10.9.1",
        "type-fest": "2.19.0",
        "typescript": "4.8.4"
    },
    "dependencies": {
        "@fastify/auth": "4.1.0",
        "@fastify/cookie": "8.3.0",
        "@fastify/jwt": "6.3.3",
        "@fastify/multipart": "7.3.0",
        "@fastify/static": "6.5.0",
        "@fastify/swagger": "8.1.0",
        "@fastify/swagger-ui": "1.2.0",
        "@prisma/client": "4.6.1",
        "@segment/ajv-human-errors": "2.1.2",
        "@sinclair/typebox": "0.25.8",
        "ajv": "8.11.2",
        "ajv-formats": "2.1.1",
        "ajv-keywords": "5.1.0",
        "argon2": "0.30.2",
        "dotenv": "16.0.3",
        "fastify": "4.10.0",
        "fastify-plugin": "4.3.0",
        "fastify-socket.io": "4.0.0",
        "fs-extra": "10.1.0",
        "http-errors": "2.0.0",
        "nodemailer": "6.8.0",
        "sharp": "0.31.2",
        "socket.io": "4.5.3",
        "socketio-jwt-auth": "0.2.1",
        "tslib": "2.4.1",
        "uuid": "9.0.0"
    }
}
